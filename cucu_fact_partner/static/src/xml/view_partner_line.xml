<templates id="template" xml:space="preserve">
    <t t-inherit="point_of_sale.PartnerListScreen" t-inherit-mode="extension">
      <xpath expr="//table//thead//tr//th[1]" position="after">
          <th class="py-2">Nit Client</th>
      </xpath>
    </t>
    <t t-inherit="point_of_sale.PartnerLine" t-inherit-mode="extension">
      <xpath expr="//tr//td[1]" position="after">
          <td>
                <div class="" t-if="props.partner.nit_client">
                    <t t-esc="props.partner.nit_client"/>
                </div>
                <div class="" t-else="">
                    -
                </div>
            </td>
      </xpath>
    </t>
</templates>






